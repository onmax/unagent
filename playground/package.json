{
  "name": "unagent-playground",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nitro dev",
    "dev:app": "vite dev app --config app/vite.config.ts",
    "build:app": "vite build app --config app/vite.config.ts",
    "build:vercel": "nitro build --preset vercel",
    "build:cf": "pnpm build:app && nitro build --preset cloudflare_module",
    "dev:vercel": "vercel dev --yes",
    "deploy:vercel": "vercel deploy .. --yes",
    "dev:cf": "pnpm build:cf && wrangler dev",
    "deploy:cf": "pnpm build:cf && wrangler deploy"
  },
  "dependencies": {
    "@cloudflare/sandbox": "catalog:sandbox",
    "@openworkflow/backend-postgres": "catalog:",
    "@vercel/queue": "catalog:",
    "@vercel/sandbox": "catalog:sandbox",
    "h3": "catalog:",
    "nitropack": "catalog:",
    "openworkflow": "catalog:",
    "unagent": "workspace:*",
    "workflow": "catalog:"
  },
  "devDependencies": {
    "@nuxt/ui": "catalog:",
    "@vercel/node": "catalog:sandbox",
    "@vitejs/plugin-vue": "catalog:",
    "@vueuse/core": "catalog:",
    "tailwindcss": "catalog:",
    "vite": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  }
}
