{
  "name": "unagent-playground",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nitro dev",
    "build:vercel": "nitro build --preset vercel",
    "build:cf": "nitro build --preset cloudflare_module",
    "dev:vercel": "vercel dev --yes",
    "deploy:vercel": "vercel deploy .. --yes",
    "dev:cf": "pnpm build:cf && wrangler dev",
    "deploy:cf": "pnpm build:cf && wrangler deploy"
  },
  "dependencies": {
    "@cloudflare/sandbox": "catalog:sandbox",
    "@openworkflow/backend-postgres": "catalog:",
    "@vercel/queue": "catalog:",
    "@vercel/sandbox": "catalog:sandbox",
    "h3": "catalog:",
    "nitropack": "catalog:",
    "openworkflow": "catalog:",
    "unagent": "workspace:*",
    "workflow": "catalog:"
  },
  "devDependencies": {
    "@vercel/node": "catalog:sandbox"
  }
}
