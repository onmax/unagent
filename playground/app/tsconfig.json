{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": { "#build/ui": ["./node_modules/.nuxt-ui/ui"] },
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.vue", "auto-imports.d.ts", "components.d.ts"]
}
