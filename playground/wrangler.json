{
  "name": "unagent-playground",
  "main": "src/worker.ts",
  "assets": { "directory": ".output/public" },
  "compatibility_date": "2026-02-05",
  "compatibility_flags": ["nodejs_compat"],
  "browser": { "binding": "MYBROWSER" },
  "account_id": "ba6e044dfdaaffec3cac45e9feccd237",
  "containers": [{
    "class_name": "Sandbox",
    "image": "./Dockerfile.sandbox",
    "max_instances": 3
  }],
  "durable_objects": {
    "bindings": [
      { "name": "SANDBOX", "class_name": "Sandbox" }
    ]
  },
  "migrations": [
    { "tag": "v1", "new_sqlite_classes": ["Sandbox"] }
  ],
  "workflows": [{
    "binding": "MY_WORKFLOW",
    "name": "unagent-playground-workflow",
    "class_name": "DemoWorkflow"
  }],
  "queues": {
    "producers": [{ "binding": "MY_QUEUE", "queue": "unagent-playground-queue" }]
  },
  "vectorize": [{
    "binding": "VECTORIZE",
    "index_name": "unagent-playground-vector"
  }]
}
