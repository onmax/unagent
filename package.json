{
  "name": "unagent",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.28.2",
  "description": "Unified agent primitives for AI coding agents",
  "author": "Maximilian Conradt <https://github.com/onmax>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/onmax",
  "homepage": "https://unagent.onmax.me",
  "repository": { "type": "git", "url": "https://github.com/onmax/unagent" },
  "keywords": ["unjs", "agent", "ai", "skills", "claude", "cursor", "windsurf"],
  "sideEffects": false,
  "exports": {
    ".": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
    "./context": { "types": "./dist/context/index.d.ts", "default": "./dist/context/index.js" },
    "./env": { "types": "./dist/env/index.d.ts", "default": "./dist/env/index.js" },
    "./exec": { "types": "./dist/exec/index.d.ts", "default": "./dist/exec/index.js" },
    "./fs": { "types": "./dist/fs/index.d.ts", "default": "./dist/fs/index.js" },
    "./git": { "types": "./dist/git/index.d.ts", "default": "./dist/git/index.js" },
    "./hooks": { "types": "./dist/hooks/index.d.ts", "default": "./dist/hooks/index.js" },
    "./link": { "types": "./dist/link/index.d.ts", "default": "./dist/link/index.js" },
    "./lock": { "types": "./dist/lock/index.d.ts", "default": "./dist/lock/index.js" },
    "./registry": { "types": "./dist/registry/index.d.ts", "default": "./dist/registry/index.js" },
    "./sandbox": { "types": "./dist/sandbox/index.d.ts", "default": "./dist/sandbox/index.js" },
    "./workflow": { "types": "./dist/workflow/index.d.ts", "default": "./dist/workflow/index.js" },
    "./skill": { "types": "./dist/skill/index.d.ts", "default": "./dist/skill/index.js" },
    "./source": { "types": "./dist/source/index.d.ts", "default": "./dist/source/index.js" },
    "./stop": { "types": "./dist/stop/index.d.ts", "default": "./dist/stop/index.js" },
    "./stream": { "types": "./dist/stream/index.d.ts", "default": "./dist/stream/index.js" },
    "./tool": { "types": "./dist/tool/index.d.ts", "default": "./dist/tool/index.js" },
    "./usage": { "types": "./dist/usage/index.d.ts", "default": "./dist/usage/index.js" },
    "./utils": { "types": "./dist/utils/index.d.ts", "default": "./dist/utils/index.js" }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "release": "bumpp",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@cloudflare/sandbox": ">=0.1.0",
    "@vercel/sandbox": ">=0.1.0",
    "unstorage": ">=1.10.0"
  },
  "peerDependenciesMeta": {
    "@cloudflare/sandbox": { "optional": true },
    "@vercel/sandbox": { "optional": true },
    "unstorage": { "optional": true }
  },
  "dependencies": {
    "hookable": "catalog:prod",
    "pathe": "catalog:prod",
    "std-env": "catalog:prod",
    "yaml": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@types/node": "catalog:types",
    "bumpp": "catalog:script",
    "eslint": "catalog:lint",
    "lint-staged": "catalog:lint",
    "simple-git-hooks": "catalog:lint",
    "tsdown": "catalog:build",
    "typescript": "catalog:types",
    "vitest": "catalog:test"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
