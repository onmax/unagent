{
  "name": "unagent",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.28.2",
  "description": "Unified agent primitives for AI coding agents",
  "author": "Maximilian Conradt <https://github.com/onmax>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/onmax",
  "homepage": "https://unagent.onmax.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/onmax/unagent"
  },
  "keywords": [
    "unjs",
    "agent",
    "ai",
    "skills",
    "claude",
    "cursor",
    "windsurf"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "default": "./dist/env/index.js"
    },
    "./fs": {
      "types": "./dist/fs/index.d.ts",
      "default": "./dist/fs/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./jobs": {
      "types": "./dist/jobs/index.d.ts",
      "default": "./dist/jobs/index.js"
    },
    "./task": {
      "types": "./dist/task/index.d.ts",
      "default": "./dist/task/index.js"
    },
    "./queue": {
      "types": "./dist/queue/index.d.ts",
      "default": "./dist/queue/index.js"
    },
    "./sandbox": {
      "types": "./dist/sandbox/index.d.ts",
      "default": "./dist/sandbox/index.js"
    },
    "./workflow": {
      "types": "./dist/workflow/index.d.ts",
      "default": "./dist/workflow/index.js"
    },
    "./skill": {
      "types": "./dist/skill/index.d.ts",
      "default": "./dist/skill/index.js"
    },
    "./usage": {
      "types": "./dist/usage/index.d.ts",
      "default": "./dist/usage/index.js"
    },
    "./vector": {
      "types": "./dist/vector/index.d.ts",
      "default": "./dist/vector/index.js"
    },
    "./vector/adapters/*": {
      "types": "./dist/vector/adapters/*.d.ts",
      "default": "./dist/vector/adapters/*.js"
    },
    "./vector/embeddings": {
      "types": "./dist/vector/embeddings/index.d.ts",
      "default": "./dist/vector/embeddings/index.js"
    },
    "./vector/embeddings/*": {
      "types": "./dist/vector/embeddings/*.d.ts",
      "default": "./dist/vector/embeddings/*.js"
    },
    "./vercel": {
      "types": "./dist/vercel/index.d.ts",
      "default": "./dist/vercel/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:dts:optional-peer": "pnpm -s build && ! rg -n \"from ['\\\\\\\"](@netlify/async-workloads|@vercel/queue|@pinecone-database/pinecone|@qdrant/js-client-rest|weaviate-client|@upstash/vector|playwright|@browserbasehq/sdk|@cloudflare/playwright|@vercel/sandbox|@cloudflare/sandbox)['\\\\\\\"]|import\\\\(['\\\\\\\"](@netlify/async-workloads|@vercel/queue|@pinecone-database/pinecone|@qdrant/js-client-rest|weaviate-client|@upstash/vector|playwright|@browserbasehq/sdk|@cloudflare/playwright|@vercel/sandbox|@cloudflare/sandbox)['\\\\\\\"]\\\\)\" dist/*.d.ts dist/**/*.d.ts",
    "pricing:sync": "node scripts/sync-ai-gateway-pricing.mjs",
    "playground:vectorize:smoke": "node playground/scripts/vectorize-smoke.mjs",
    "playground:browser:e2e": "pnpm build && node playground/scripts/browser-e2e.mjs",
    "playground:browser:e2e:docker": "docker compose -f playground/docker-compose.browser.yml run --rm --build playwright-e2e",
    "playground:jobs:netlify:e2e": "node playground/scripts/jobs-netlify-e2e.mjs",
    "playground:queue:netlify:e2e": "node playground/scripts/queue-netlify-e2e.mjs",
    "playground:vector:e2e": "pnpm build && node playground/scripts/vector-e2e.mjs",
    "playground:workflow:e2e": "node playground/scripts/openworkflow-e2e.mjs",
    "prepack": "node scripts/prepare-pack-manifest.mjs",
    "postpack": "node scripts/restore-pack-manifest.mjs",
    "release": "bumpp",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@ai-sdk/cohere": ">=0.0.0",
    "@ai-sdk/google": ">=0.0.0",
    "@ai-sdk/mistral": ">=0.0.0",
    "@ai-sdk/openai": ">=0.0.0",
    "@browserbasehq/sdk": ">=0.0.0",
    "@cloudflare/playwright": ">=0.0.0",
    "@cloudflare/sandbox": ">=0.1.0",
    "@deno/sandbox": ">=0.1.0",
    "@huggingface/transformers": ">=0.0.0",
    "@libsql/client": ">=0.0.0",
    "@netlify/async-workloads": ">=0.0.0",
    "@pinecone-database/pinecone": ">=0.0.0",
    "@qdrant/js-client-rest": ">=0.0.0",
    "@upstash/qstash": ">=0.0.0",
    "@upstash/vector": ">=1.0.0",
    "@vercel/queue": ">=0.0.0-0",
    "@vercel/sandbox": ">=0.1.0",
    "ai": ">=3.0.0",
    "ollama-ai-provider-v2": ">=0.0.0",
    "openworkflow": ">=0.0.0",
    "pg": ">=8.0.0",
    "playwright": ">=0.0.0",
    "sqlite-vec": ">=0.0.0",
    "unstorage": ">=1.10.0",
    "weaviate-client": ">=0.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/cohere": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/mistral": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@browserbasehq/sdk": {
      "optional": true
    },
    "@cloudflare/playwright": {
      "optional": true
    },
    "@cloudflare/sandbox": {
      "optional": true
    },
    "@deno/sandbox": {
      "optional": true
    },
    "@huggingface/transformers": {
      "optional": true
    },
    "@libsql/client": {
      "optional": true
    },
    "@netlify/async-workloads": {
      "optional": true
    },
    "@pinecone-database/pinecone": {
      "optional": true
    },
    "@qdrant/js-client-rest": {
      "optional": true
    },
    "@upstash/qstash": {
      "optional": true
    },
    "@upstash/vector": {
      "optional": true
    },
    "@vercel/queue": {
      "optional": true
    },
    "@vercel/sandbox": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "ollama-ai-provider-v2": {
      "optional": true
    },
    "openworkflow": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "playwright": {
      "optional": true
    },
    "sqlite-vec": {
      "optional": true
    },
    "unstorage": {
      "optional": true
    },
    "weaviate-client": {
      "optional": true
    }
  },
  "dependencies": {
    "ajv": "catalog:prod",
    "croner": "catalog:prod",
    "hookable": "catalog:prod",
    "pathe": "catalog:prod",
    "std-env": "catalog:prod",
    "yaml": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@libsql/client": "catalog:vector",
    "@pinecone-database/pinecone": "catalog:vector",
    "@qdrant/js-client-rest": "catalog:vector",
    "@types/node": "catalog:types",
    "@upstash/vector": "catalog:vector",
    "bumpp": "catalog:script",
    "eslint": "catalog:lint",
    "jiti": "catalog:lint",
    "lint-staged": "catalog:lint",
    "pg": "catalog:vector",
    "simple-git-hooks": "catalog:lint",
    "sqlite-vec": "catalog:vector",
    "tsdown": "catalog:build",
    "type-fest": "catalog:types",
    "typescript": "catalog:types",
    "vitest": "catalog:test",
    "weaviate-client": "catalog:vector"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
